<div id="index-banner" class="parallax-container">
  <div class="section">
    <div class="container">
      <br><br><br>
      <p class="center bits-main">Meet Your Neighbors</p>
      <div class="row center">
      </div>
      <br><br>
    </div>
  </div>
  <div class="parallax"><img src="/images/laughs.jpg"></div>
</div>
<div class="row">
  <p class="center bits-comedians">All Comedians</h5>
</div>
<div class="center container all-comedian-area">
  <div class="row center">
  <div class="center"></div>
    {{#each recent}}
      <div class="col center">
        <div class="card center" id="new-comedian">
          <div class="card-image">
            <img id="profile-photo" src="{{ photo }}">
          </div>
          <div class="card-content" id="text-aligned-center">
            <span class="card-title">{{ name }}</span>
          </div>
          <div class="card-action" id="text-aligned-center">
            <a href="/users/{{username}}">Meet them here!</a>
            <form action="/" method="GET" class="form-inline">
          </div>
        </div>
      </div>
    {{/each}}
  </div>
  <div class="row">
    <br><br>
  </div>
</div>